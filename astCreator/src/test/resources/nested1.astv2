Packages
base org.overture.ast.node;
analysis org.overture.ast.analysis;
Tokens
  int = 'int';
 
 
Abstract Syntax Tree
exp {-> package='org.overture.ast.expressions'}
    =   {aa} [type]:stm.#stm2.z
    ;
 
stm {-> package='org.overture.ast.statement'}
    =   {st1} 
    |   {st2}
    |   #stm2
    ;
    
#stm2 {-> package='org.overture.ast.statement'}
    =   {z}
    ;
